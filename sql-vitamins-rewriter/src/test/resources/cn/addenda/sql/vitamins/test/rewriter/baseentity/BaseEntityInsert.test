insert into score(name) select a from A union select b from (select b from B) B2 union select c from C; insert into score (name, creator, creator_name, create_time, modifier , modifier_name, modify_time, remark) select a, A.creator as creator, A.creator_name as creator_name, A.create_time as create_time, A.modifier as modifier , A.modifier_name as modifier_name, A.modify_time as modify_time, A.remark as remark from A union select b, B2.B_creator as creator, B2.B_creator_name as creator_name, B2.B_create_time as create_time, B2.B_modifier as modifier , B2.B_modifier_name as modifier_name, B2.B_modify_time as modify_time, B2.B_remark as remark from ( select b, B.creator as B_creator, B.creator_name as B_creator_name, B.create_time as B_create_time, B.modifier as B_modifier , B.modifier_name as B_modifier_name, B.modify_time as B_modify_time, B.remark as B_remark from B ) B2 union select c, C.creator as creator, C.creator_name as creator_name, C.create_time as create_time, C.modifier as modifier , C.modifier_name as modifier_name, C.modify_time as modify_time, C.remark as remark from C
